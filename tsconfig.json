{
    "compilerOptions": {
      "target": "es2018",
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./middleware/datasource/src"
    },
    "include": ["middleware/datasource/src/**/*"],
    "exclude": ["node_modules", "**/*.spec.ts"]
  }